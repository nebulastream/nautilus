{
  "image": "mcr.microsoft.com/devcontainers/cpp:ubuntu-24.04",
  "name": "Try Nautilus",
  "customizations": {
    "vscode": {
      "extensions": [
        "matepek.vscode-catch2-test-adapter"
      ]
    }
  },
  "containerEnv": {
    "CC": "clang-19",
    "CXX": "clang++-19"
  },
  "postCreateCommand": "bash .devcontainer/setup.sh"
}
